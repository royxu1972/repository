<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Combinatorial testing repository is a web application to collect all publications that are related to combinatorial testing.">
    <meta name="keywords" content="Repository, Combinatorial Testing, Software Testing, Publication, Research, Paper">
    <meta name="author" content="Huayao">

    <title>Publication Venue</title>
    <link rel="stylesheet" href="http://gist.nju.edu.cn/css/bootstrap.min.css">
    <link rel="stylesheet" href="css/font-awesome.min.css">
    <link rel="stylesheet" href="css/repository.css">
    
    <script src="http://gist.nju.edu.cn/js/jquery.min.js"></script>
    <script src="js/repository.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            // search field id
            $("p.vp a[id]").click(function () {
                start = 0;
                ct_url = 'action/query';
                ct_para = {start: start, step: step, group: 'booktitle', content: $(this).attr("id")};
                search_content = $(this).text();
                search_type = "venue";
                $("#main_list").hide();
                makeRequest();
            });
        });
    </script>
</head>

<body>
<!-- Wrap all page content here -->
<div id="wrap">

    <!-- top navigation -->
    <%- include template/template_top.ejs %>

    <div class="container">
        <br/><br/>
        <br/><br/>

        <div class="row">
            <!-- sidebar -->
            <%- include template/template_sidebar.ejs %>

            <!-- show -->
            <div class="col-md-10 col-sm-9 list_col">
                <!-- main list -->
                <div id="main_list" class="row">
                    <div class="col-md-12">
                        <h4>Refereed Journals</h4>
                        <hr>
                        <% article.forEach(function(each) { %>
                        <p class="vp"><a id="<%=each.abbr%>" href='#'><%=each.booktitle%> (<%=each.abbr%>)</a></p>
                        <% }); %>
                        <br/><br/>

                        <h4>Refereed Conferences</h4>
                        <hr>
                        <% inproceeding.forEach(function(each) { %>
                        <p class="vp"><a id="<%=each.abbr%>" href='#'><%=each.booktitle%> (<%=each.abbr%>)</a></p>
                        <% }); %>
                        <br/><br/>

                        <h4>Others</h4>
                        <hr>
                        <p class="vp"><a id="phdthesis" href="#">Ph.D. and Master Dissertation</a></p>
                        <p class="vp"><a id="book" href="#">Book and Chapter</a></p>
                        <p class="vp"><a id="techreport" href="#">Technical Report</a></p>
                        <br/><br/>
                    </div>
                </div>

                <!-- result list -->
                <%- include template/template_result.ejs %>
            </div>
            <!-- /show -->
        </div>
    </div>
    <!-- /container -->

    <div id="push"></div>
</div>
<!-- /wrap -->

<!-- footer -->
<%- include template/template_bottom.ejs %>

<script src="http://gist.nju.edu.cn/js/bootstrap.min.js"></script>
</body>
</html>
